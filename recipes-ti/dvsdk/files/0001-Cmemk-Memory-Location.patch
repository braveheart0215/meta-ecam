From: Garrett <g@floft.net>
Date: Fri, 16 Aug 2013 08:30:55 -0700
Subject: [PATCH 1/3] Cmemk Memory Location

---
 etc/init.d/loadmodule-rc |    5 +----
 1 files changed, 1 insertions(+), 4 deletions(-)

diff --git a/etc/init.d/loadmodule-rc b/etc/init.d/loadmodule-rc
index a8f8fb9..f133dd4 100755
--- a/etc/init.d/loadmodule-rc
+++ b/etc/init.d/loadmodule-rc
@@ -9,13 +9,10 @@
 # 0x85900000     39 MB  CODEC SERVER
 
 load () {
-    modprobe cmemk phys_start=0x83700000 phys_end=0x85900000 allowOverlap=1 useHeapIfPoolUnavailable=1
+    modprobe cmemk phys_start=0x9b700000 phys_end=0x9d900000 allowOverlap=1 useHeapIfPoolUnavailable=1 pools=1x10485760,6x2097152,1x691200,1x1048576,1x1
     modprobe dsplinkk
     modprobe lpm_omap3530
     modprobe sdmak
-    amixer cset name='HeadsetL Mixer AudioL1' on
-    amixer cset name='HeadsetR Mixer AudioR1' on
-    amixer -c 0 set Headset 1+ unmute 
 }
 
 unload () {
